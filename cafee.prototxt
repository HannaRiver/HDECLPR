name:"chejiahao_Pose_Estimation"
layer {
  name: "img_input"
  type: "ImageData"
  top: "data"
  top: "pose_label"
  include {
    phase:TRAIN
  }
    image_data_param {
    #root_folder: "/home/em-gkj/devdata/multilabel/data/"
    source: "/home/em-gkj/devdata/xuchen/chejiahao/all_label_train.txt"
    batch_size: 16
    new_height: 128
    new_width: 512
    is_color: true
    shuffle: true
    num_of_labels: 2
  }
  transform_param
  {
    #mean_value :127.5
    #scale : 0.0078125
  }
}
layer {
  name: "img_input"
  type: "ImageData"
  top: "data"
  top: "pose_label"
  include {
    phase: TEST
  }
    image_data_param {
    #root_folder: "/home/em-gkj/devdata/multilabel/data/"
    source: "/home/em-gkj/devdata/xuchen/chejiahao/all_label_test.txt"
    batch_size: 4
    new_height: 128
    new_width: 512
    is_color: true
    shuffle: true
    num_of_labels: 2
  }
    transform_param
    {
    #mean_value : 127.5
    #scale : 0.0078125
  }
}
layer {
  bottom: "data"
  top: "conv1_1"
  name: "conv1_1"
  type: "Convolution"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    weight_filler {
      type: "msra"
    }
    bias_filler {
      type: "constant"
    }
    num_output: 64
    pad: 1
    kernel_size: 3
  }
}
layer {
  bottom: "conv1_1"
  top: "conv1_1"
  name: "relu1_1"
  type: "ReLU"
}
layer {
  bottom: "conv1_1"
  top: "conv1_2"
  name: "conv1_2"
  type: "Convolution"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    weight_filler {
      type: "msra"
    }
    bias_filler {
      type: "constant"
    }
    num_output: 64
    pad: 1
    kernel_size: 3
  }
}

layer {
  bottom: "conv1_2"
  top: "conv1_2"
  name: "relu1_2"
  type: "ReLU"
}
layer {
  bottom: "conv1_2"
  top: "pool1"
  name: "pool1"
  type: "Pooling"
  pooling_param {
    pool: MAX
    kernel_size: 2
    stride: 2
  }
}
layer {
  bottom: "pool1"
  top: "conv2_1"
  name: "conv2_1"
  type: "Convolution"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    weight_filler {
      type: "msra"
    }
    bias_filler {
      type: "constant"
    }
    num_output: 64
    pad: 1
    kernel_size: 3
  }
}

layer {
  bottom: "conv2_1"
  top: "conv2_1"
  name: "relu2_1"
  type: "ReLU"
}
layer {
  bottom: "conv2_1"
  top: "conv2_2"
  name: "conv2_2"
  type: "Convolution"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    weight_filler {
      type: "msra"
    }
    bias_filler {
      type: "constant"
    }
    num_output: 64
    pad: 1
    kernel_size: 3
  }
}

layer {
